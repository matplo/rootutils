#%Module
proc ModulesHelp { } {
        global version
        puts stderr "   Setup pyroot utils $version"
    }

set version 3.7
setenv PYROOTUTILS $env(HOME)/devel/rootutils/python/3.7
prepend-path PATH $env(HOME)/devel/rootutils/python/3.7
prepend-path PYTHONPATH $env(HOME)/devel/rootutils/python/3.7
# prepend-path PATH $env(HOME)/devel/rootutils/python/3.7/mdown
# prepend-path PYTHONPATH $env(HOME)/devel/rootutils/python/3.7/mdown
# prepend-path PYTHONPATH /usr/local/lib/python3.7/site-packages
set-alias pyroot_cd "cd $env(HOME)/devel/rootutils/python/3.7"
